{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "importHelpers": true,
      "noImplicitAny": false,
      "jsx": "react",
      "esModuleInterop": true,
      "sourceMap": true,
      "baseUrl": "./",
      "strict": false,
      "paths": {
        "@/*": ["src/*"],
      },
      "allowSyntheticDefaultImports": true
    },
    "include": [
      "mock/**/*",
      "src/**/*",
      "config/**/*",
      ".umirc.ts",
      "typings.d.ts"
    ]
  }
